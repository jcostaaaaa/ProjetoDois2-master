CREATE TABLE "USERS"
   (    "ID_USER" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY,
    "EMAIL" VARCHAR2(100 BYTE) NOT NULL ENABLE,
    "PASSWORD" VARCHAR2(100 BYTE) NOT NULL ENABLE,
    "IS_ADMIN" NUMBER(1,0) DEFAULT 0 NOT NULL ENABLE,
    "IS_GESTOR" NUMBER(1,0) DEFAULT 0 NOT NULL ENABLE,
    "IS_ESTAFETA" NUMBER(1,0) DEFAULT 0 NOT NULL ENABLE,
    "IS_COZINHEIRO" NUMBER(1,0) DEFAULT 0 NOT NULL ENABLE,
    "NOMEUSER" VARCHAR2(20 BYTE) NOT NULL ENABLE,
     CONSTRAINT "USER_PK" PRIMARY KEY ("ID_USER") );



ALTER TABLE ESTAFETA
ADD id_user NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY
CONSTRAINT id_user_FK REFERENCES USERS(ID_USER);
